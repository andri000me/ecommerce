{% extends 'application.twig' %}

{% block content %}
	<h1>{{ title }}</h1>
	{% for error in errors %}
		<div class="alert alert-danger">
			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
			<P>{{ error }}</P>
		</div>
	{% endfor %}	
	{% include '_form_customer.twig' %}
{% endblock %}