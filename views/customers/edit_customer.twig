{% extends 'application_admin.twig' %}

{% block content %}
	{% for error in errors %}
		<div class="alert alert-danger">
			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
			<P>{{ error }}</P>
		</div>
	{% endfor %}
	{% for result in results %}
		{% include '_form_customer.twig' %}
	{% endfor %}
{% endblock %}